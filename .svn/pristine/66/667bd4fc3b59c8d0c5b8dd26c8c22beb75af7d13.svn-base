                                , { field: 'CncID', title: '机床ID', width: '10%',hide:true }
                                , {
                                    field: 'do', title: '操作', width: '10%', templet: function (d) {
                                        return'<button class="layui-btn layui-btn-sm">受理</button>'
                                    }
                                }
                                

                            ]],//
                            done: function (dd) {
