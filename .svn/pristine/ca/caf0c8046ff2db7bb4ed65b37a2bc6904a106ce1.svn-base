    <script>

        $(function () {
            $("#test1").click(function () {
                layui.use('layer', function () {
                    layer.open({
                        type: 2,
                        area: ['70%', '70%'],
                        content: '品质检测导表/导表弹窗.html',
                        success: function (layero, index) {
                            var iframeWin = window[layero.find('iframe')[0]['name']]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                            iframeWin.method(orderId)
                        }
                        , end: function () {
                            location.reload()
                        }
                    });
                })
                // $("#file").click();
            })
            $("#file").change(function () {
                var formData = new FormData();

                formData.append('file', $("#file")[0].files[0])
                $.ajax({
                    url: 'importExcel.ashx',
                    type: 'post',
                    contentType: false,
                    processData: false,
                    data: formData,
                    success: function (d) {
                        if (d == "ok") {
                            layui.use('layer', function () {
                                layui.layer.alert("导入成功")
                            })

                        } else {
                            alert(d)
                        }
                    }
                })
            })
        })

        layui.use(['table', 'layer'], function () {
            var table = layui.table, layer = layui.layer;//头工具栏事件
            table.on('toolbar(test)', function (obj) {
                var checkStatus = table.checkStatus('idTest'); //idTest 即为基础参数 id 对应的值
                switch (obj.event) {
                    case 'add':
                        layer.open({
                            type: 2,
                            area: ['70%', '70%'],
                            content: '添加尺寸.html',
                            success: function (layero, index) {
                              
                                var iframeWin = window[layero.find('iframe')[0]['name']]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                                iframeWin.method(orderId)
                            }

                        });
                        break;
                    case 'del':
                        if (checkStatus.data.length == 0) {
                            layer.alert("清先勾选");
                            return;
                        }
                        layer.confirm("是否删除", function (index) {
                            var formData = new FormData();
                            for (var i = 0; i < checkStatus.data.length; i++) {
                                formData.append('ids', checkStatus.data[i].Id);

                            }
                            $.ajax({
                                url: '品质检测手动处理/批量删除尺寸.ashx',
                                type: 'post',
                                contentType: false,
                                processData: false,
                                data: formData,
                                success: function (d) {
                                    if (d == "ok") {
                                        layer.msg('删除成功', { icon: 1, time: 1000 }, function () {
                                            reload.reload({});
                                           // location.reload()
                                        })
                                    }
                                }
                            })
                        })

                };
            });
            //监听行工具事件
            table.on('tool(test)', function (obj) {
                var data = obj.data;
                //console.log(obj)
                if (obj.event === 'del') {
                    layer.confirm('是否删除', function (index) {

                        $.ajax({
                            url: '手动录入删除.ashx?id=' + data.ID,
                            success: function (d) {
                                if (d == "ok") {
                                    layer.msg('删除成功', { icon: 1, time: 1000 }, function () {
                                        reload.reload({})
                                    })
                                }
                            }


                        })
                        layer.close(index);
                    });
                } else if (obj.event === 'edit') {
                    layer.open({
                        type: 2,
                        area: ['70%', '70%'],
                        content: 'sizeEdit.html',
                        success: function (layero, index) {

                            var iframeWin = window[layero.find('iframe')[0]['name']]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                            iframeWin.method(data.ID)
                        }
                    });
                }
            });

        })
    </script>
    <script type="text/html" id="toolbarDemo">
        <div class="layui-btn-container">
            <button class="layui-btn layui-btn-sm" lay-event="add">添加尺寸</button>
            <button class="layui-btn layui-btn-sm layui-btn-danger" lay-event="del">批量删除</button>
        </div>
        <input id="orderId" hidden />
   
    </script>
    <script type="text/html" id="barDemo">
        <a class="layui-btn layui-btn-xs" lay-event="edit">编辑</a>
        <a class="layui-btn layui-btn-danger layui-btn-xs" lay-event="del">删除</a>
    </script>
