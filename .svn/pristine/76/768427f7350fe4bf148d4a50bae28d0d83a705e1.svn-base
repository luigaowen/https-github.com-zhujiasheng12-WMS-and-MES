                        <th id="Product_Output" colspan="4"></th>
                    </tr>
                    <tr>
                        <th>项目名称</th>
                        <th id="ProjectName" colspan="5"></th>
                    </tr>
                    <tr>
                        <th>客户名称</th>
                        <th id="ClientName" colspan="5"></th>

                    </tr>
                    <tr>
                        <th>毛坯材料</th>
                        <th id="Product_Material" colspan="1" style="width:500px"> </th>
                        <th style="width:200px;display:none">
                        <th style="display:none"></th>
                        <th style="width:100px">毛坯规格</th>
                        <th id="BlankSpecification" colspan="3"></th>
                    </tr>
                    <tr>
                        <th>订单负责人</th>
                        <th id="Order_Leader" colspan="1"></th>
                        <th>工程负责人</th>
                        <th id="craftPerson" colspan="3"></th>
                    </tr>
                    <tr>
                        <th colspan="6">工序信息</th>
                    </tr>
                </thread>
            </table>
            <table class="table table-bordered">
                <thead>
                    <tr>
                        <th>工序</th>
                        <th>设备类型</th>
                        <th>治具类型</th>
                        <th>有无特殊刀具</th>
                        <th>装夹示意图</th>
                        <th>备注</th>
                    </tr>
                </thead>
                <tbody id="process">
                    <!--    <tr><td>一序</td><td>JDVT600_A12S真空吸盘</td><td>真空吸盘</td><td>无</td><td><img  src="获取装夹示意图.ashx" /></td><td></td></tr>
                 <tr><td>二序</td><td>JDLVM550T_A12S 小五轴</td><td>快换式斜口台钳L130(钳口深度3mm，可夹持宽度26-115mm)</td><td>有</td><td></td><td></td></tr>-->
                </tbody>
            </table>
        </div>
    </div>

    <script>
        var qrcode = new QRCode(document.getElementById("qrcode"), {
            width: 100,
            height: 100
        });


        function print1() {
            // window.print()

            var printHtml = document.getElementById('word').innerHTML

            var oldstr = document.body.innerHTML
            window.document.body.innerHTML = printHtml
            window.print()
            document.body.innerHTML = oldstr
            location.reload()
            return false
        }

