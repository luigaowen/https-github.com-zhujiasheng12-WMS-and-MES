    <script>
        layui.use('table', function () {
            var table = layui.table;

            //第一个实例
            reload = table.render({
                elem: '#demo',
                toolbar: '#toolbar',
                url: '毛坯管理读数据.ashx' //数据接口
                    ,
                page: true //开启分页
                    ,
                cols: [
                    [ //表头
                        {
                            field: 'order',
                            title: '订单号',
                            width: 120,
                            fixed: 'left'
                        }, {
                            field: 'projectName',
                            title: '项目名称',
                            width: 120,
                        }, {
                            field: 'orderName',
                            title: '产品名称',
                            width: 120,
                        }, {
                            field: 'blankType',
                            title: '毛坯种类',
                            width: 90,
                            templet: function (d) {
                                return '<p style="color:blue">' + d.blankType + '</p>'
                            }
                        }, {
                            field: 'blankSpecification',
                            title: '毛坯规格',
                            width: 150,
                        }, {
                            field: 'blankDemandNumber',
                            title: '毛坯需求数量',
                            width: 120
                        }, {
                            field: 'orderUsedNumber',
                            title: '订单已使用量',
                            width: 120
                        }, {
                            field: 'orderFinishedProductNumber',
                            title: '订单成品量',
                            width: 100,
                        }, {
                            field: 'orderWasteNumber',
                            title: '订单废品量',
                            width: 100,
                        }, {
                            field: 'orderGoodProductRate',
                            title: '订单良品率',
                            width: 100
                        }, {
                            field: 'blankSparePartsState',
                            title: '毛坯备料状态',
                            width: 135,
                        }, {
                            field: 'blankPreparedNumber',
                            title: '毛坯已准备总量',
                            width: 135,
                        }, {
                            field: 'blankBolusNumber',
                            title: '毛坯追加量',
                            width: 135,
                        }, {
                            field: 'Expected_Completion_Time',
                            title: '预计完成日期',
                            width: 180,
                            templet: function (d) {
                                return '<p style="color:blue">' + d.Expected_Completion_Time +
                                    '</p>'
                            }
                        }, {
                            field: 'time',
                            title: '延期日期',
                            width: 180,
                            templet: function (d) {
                                return '<p style="color:blue">' + d.time + '</p>'
                            }
                        }, {
                            field: 'orderId',
                            title: '操作',
                            toolbar: '#barDemo',
                            width: 180,
                            fixed: 'right'
                        }, {
                            field: 'orderId1',
                            title: 'orderId',
                            hide: true
                        }
                    ]
                ],
                parseData: function (res) { //res 即为原始返回的数据
                    res = res.data
                    let current_pages;
                    if (this.page === true) current_pages = 1;
                    else current_pages = this.page.curr;
                    let sort = []
                    sort.push(...res.filter(item => item.blankSparePartsState === "待备料").sort((a,b)=>{
                        return a.order<b.order?-1:1
                    }))
                    sort.push(...res.filter(item => item.blankSparePartsState === "已完成").sort((a,b)=>{
                        return a.order<b.order?-1:1
                    }))
                    const data = sort.slice(this.limit * (current_pages - 1), this.limit *
                        current_pages);
                    return {
                        "code": 0, //解析接口状态
                        "msg": 0, //解析提示文本
                        "count": sort.length, //解析数据长度
                        "data": data //解析数据列表
                    };
                },
                done: function (d) {
                    $('td[data-field="blankType"]').click(function () {
                        var orderNum = $(this).siblings('[data-field="order"]')[0].children[
                            0].innerText


                        layer.open({
                            type: 2,
                            content: '资材毛坯图纸弹窗.html',
                            area: ['50%', '90%'],
                            success: function (layero, index) {

                                var iframeWin = window[layero.find('iframe')[0][
                                    'name'
                                ]]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                                iframeWin.method(orderNum)

                            }

                        })
                    })
                    $('td[data-field="time"]').click(function () {
                        var orderId = $(this).siblings('[data-field="orderId1"]')[0]
                            .children[0].innerText
                        var time = $(this)[0].children[0].innerText
                        layer.open({
                            type: 2,
                            area: ['50%', '50%'],
                            content: '毛坯延期弹窗.html' //这里content是一个URL，如果你不想让iframe出现滚动条，你还可以content: ['http://sentsin.com', 'no']
                                ,
                            success: function (layero, index) {

                                var iframeWin = window[layero.find('iframe')[0][
                                    'name'
                                ]]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                                iframeWin.method(orderId, time);
                            }
                        });
                    })

                    $('td[data-field="Expected_Completion_Time"]').click(function () {
                        var orderId = $(this).siblings('[data-field="orderId1"]')[0]
                            .children[0].innerText
                        var Expected_Completion_Time = $(this)[0].children[0].innerText
                        layer.open({
                            type: 2,
                            area: ['50%', '50%'],
                            content: '毛坯预计完成时间弹窗.html' //这里content是一个URL，如果你不想让iframe出现滚动条，你还可以content: ['http://sentsin.com', 'no']
                                ,
                            success: function (layero, index) {

                                var iframeWin = window[layero.find('iframe')[0][
                                    'name'
                                ]]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                                iframeWin.method(orderId,
                                    Expected_Completion_Time);
                            }
                        });
                    })
                }
            });

            //监听行工具事件
            table.on('tool(test)', function (obj) {
                var data = obj.data;
                //console.log(obj)
                if (obj.event === 'add') {
                    layer.open({
                        type: 2,
                        area: ['400px', '300px'],
                        content: '追加弹窗.html' //这里content是一个URL，如果你不想让iframe出现滚动条，你还可以content: ['http://sentsin.com', 'no']
                            ,
                        success: function (layero, index) {
                            var body = layer.getChildFrame('body', index);
                            body.find('#orderId').val(data.orderId);
                            var iframeWin = window[layero.find('iframe')[0][
                                'name'
                            ]]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                            iframeWin.method(data.blankBolusNumber);
                        }
                    });
                } else if (obj.event === 'spareParts') {
                    layer.open({
                        type: 2,
                        area: ['400px', '300px'],
                        content: '备料弹窗.html' //这里content是一个URL，如果你不想让iframe出现滚动条，你还可以content: ['http://sentsin.com', 'no']
                            ,
                        success: function (layero, index) {
                            var body = layer.getChildFrame('body', index);
                            body.find('#orderId').val(data.orderId);
                            var iframeWin = window[layero.find('iframe')[0][
                                'name'
                            ]]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                            iframeWin.method(data.blankPreparedNumber);
                        }
                    });
                } else if (obj.event === 'print') {
                    var index = layer.load();
                    var orderId = data.orderId;
                    $.ajax({
                        url: './获取工单信息.ashx',
                        type: 'post',
                        data: {
                            orderId: orderId
                        },
                        success: function (res) {
                            var obj = JSON.parse(res);
                            console.log(obj)
                            $("#Product_Name").text(obj.Product_Name)

                            $("#Order_Number").text(obj.Order_Number)
                            $("#Product_Output").text(obj.Product_Output)
                            $("#ProjectName").text(obj.ProjectName)
                            $("#ClientName").text(obj.ClientName)
                            $("#Order_Leader").text(obj.Order_Leader)
                            $("#craftPerson").text(obj.craftPerson)
                            $("#Product_Material").text(obj.Product_Material)
                            $("#BlankSpecification").text(obj.BlankSpecification)

                            var date = new Date();
                            var time = '日期:' + date.getFullYear() + '/' + (date.getMonth() +
                                1) + '/' + date.getDate()
                            $("#date").text(time)
                            $("#process").empty()
                            for (var i = 0; i < obj.process.length; i++) {
                                $("#process").append('<tr><td>' + obj.process[i]
                                    .processNum + '序</td><td>' + obj.process[i]
                                    .DeviceType + '</td><td>' + obj.process[i]
                                    .JigSpecification + '</td><td>' + obj.process[i]
                                    .tool +
                                    '</td><td><img style="width:400px;height:400px" src="获取装夹示意图.ashx?orderNum=' +
                                    obj.Order_Number + '&processNum=' + obj.process[i]
                                    .processNum + '" /></td><td></td></tr>')
                            }
                            layer.close(index)
                            qrcode.makeCode(data.order);
                            setTimeout('print1()', 100);
                        }
                    })

                }
                //else if (obj.event == 'delay') {
                //    layer.open({
                //        type: 2,
                //        area: ['50%', '50%'],
                //        content: '毛坯延期弹窗.html' //这里content是一个URL，如果你不想让iframe出现滚动条，你还可以content: ['http://sentsin.com', 'no']
                //        , success: function (layero, index) {
                //            var body = layer.getChildFrame('body', index);
                //            body.find('#orderId').val(data.orderId);
                //            var iframeWin = window[layero.find('iframe')[0]['name']]; //得到iframe页的窗口对象，执行iframe页的方法：iframeWin.method();
                //            iframeWin.method(data.blankBolusNumber);
                //        }
                //    });
                //}
            });

        });

        /**/
    </script>
    <script type="text/html" id="barDemo">
        <a class="layui-btn layui-btn-xs" lay-event="spareParts" style="color:white">备料</a>
        <a class="layui-btn layui-btn-xs layui-btn-danger" lay-event="add" style="color:white">追加</a>
        <a class="layui-btn layui-btn-xs " lay-event="print" style="color:white">打印</a>
    </script>

    <script type="text/html" id="toolbar">
        <button class="layui-btn layui-btn-sm" onclick="location.reload()"><i
                class="layui-icon layui-icon-refresh-3"></i>刷新</button>
    </script>
</body>

</html>