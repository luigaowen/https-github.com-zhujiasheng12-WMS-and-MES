       <div class="layui-tab-item">

             <table id="table1" lay-filter="table1"></table>
       </div>
       <div class="layui-tab-item layui-show">

            <table id="table2" lay-filter="table2"></table>
       </div>

  </div>
</div>      
       
    <input id="searchContent" hidden />
   
</body>
