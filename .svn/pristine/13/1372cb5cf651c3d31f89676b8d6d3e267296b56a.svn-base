    <script>
     
        var myChart1 = echarts.init(document.getElementById('knifePreparationCake'));
         var myChart2 = echarts.init(document.getElementById('Cake2'));
        option1 = {

            tooltip: {
                trigger: 'item',
                formatter: "{a} <br/>{b} : {c} ({d}%)"
            },

            series: [
                {
                    name: '',
                    type: 'pie',
                    radius: '70%',

                    label: {
                        normal: {

                            formatter: '{b}:{c}',
                            textStyle: {
                                fontWeight: 'normal',
                                fontSize: 15
                            }
                        },
                        emphasis: {
                            show: true,
                            textStyle: {
                                fontSize: '30',
                                fontWeight: 'bold'
                            }
                        }
                    },
                    data: [

                        { value: 10, name: '维修中机床数', itemStyle: { color: '#FF5722' } },

                        { value: 20, name: '维修完成待确认数', itemStyle: { color: '#5FB878' } },

                    ],
                    itemStyle: {
                        emphasis: {
                            shadowBlur: 10,
                            shadowOffsetX: 0,
                            shadowColor: 'rgba(0, 0, 0, 0.5)'
                        }
                    }
                }
            ]
        };
       option2 = {

            tooltip: {
                trigger: 'item',
                formatter: "{a} <br/>{b} : {c} ({d}%)"
            },

            series: [
                {
                    name: '',
                    type: 'pie',
                    radius: '70%',

                    label: {
                        normal: {

                            formatter: '{b}:{c}',
                            textStyle: {
