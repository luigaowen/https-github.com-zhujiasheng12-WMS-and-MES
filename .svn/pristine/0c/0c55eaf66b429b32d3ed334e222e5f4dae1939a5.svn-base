<body>
    <script>

        layui.use(['table', 'layer'], function () {
              table = layui.table, layer = layui.layer, $ = layui.$;
           
             
            table.on('toolbar(test)', function (obj) {

                switch (obj.event) {
                    case 'create':

                        layer.open({
                            type: 2,
                            area: ['50%', '90%'],
                            content: '新建订单.html'
                        })
                        break;
                    case 'search':
                        $('#searchContent').val($("#select").val())
                        tableReload.reload({
                            url: 'orderEntry.ashx',
                            where: {
                                key: $("#select").val()

                            }
                        })

                        break;
                    case 'reload':
                        $('#searchContent').val("")
                        table.reload('idTest', function () {

                        })
                        break;
                };
            });

            //监听行工具事件
            table.on('tool(test)', function (obj) {
                var data = obj.data;
                //console.log(obj)
                if (obj.event === 'del') {
                    layer.confirm('是否删除', function (index) {
                        $.ajax({
                            url: 'orderDelete.ashx',
                            data: { "id": data.Order_ID },
                            success: function (d) {
                                if (d == "ok") {
                                    layer.msg("删除成功", { icon: 1, time: 1000 }, function () {
                                        layer.close(index);
                                        tableReload.reload({

                                        })
                                    })
                                } else {
                                    layer.alert(d)
                                }
                            }
                        })
                        layer.close(index);
                    });
                } else if (obj.event === 'edit') {

                    layer.open({
                        type: 2,
                        content: 'orderEdit.html',
                        area: ['50%', '80%'],
                        success: function (layero, index) {

                            var iframeWin = window[layero.find('iframe')[0]['name']];
                            iframeWin.method(data.Order_ID)
                        }
                    })
                }
            });
        })
    </script>
    <script>
        fun()
        function fun() {
            layui.use(['table', 'layer'], function () {
                table = layui.table, layer = layui.layer, $ = layui.$;
                $.ajax({

                    url: '工程安排弹窗提醒.ashx',
                    success: function (d) {
                        if (d != "") {
                            var arr = d.split(",")
                            var str = "";
                            for (var i = 0; i < arr.length; i++) {
                                str+=arr[i]+"</br>"
                            }
                            
                           
                             layer.open({
                            type: 0,
                            area: ['30%', '30%'],
                            offset: 'rb',
                            skin: 'layui-layer-lan',
                            anim: 0,
                             shade: 0 ,//不显示遮罩
                            cancel: function (index) {
                                tableReload.reload()
                            },
                            yes: function (index, layero) {
                                tableReload.reload()
                                layer.close(index); //如果设定了yes回调，需进行手工关闭
                            },
                            content: str //这里content是一个普通的String
                        });
                      
                        }
                         setTimeout('fun()', 60000)
                    }
                })
            })
        }
    </script>
