                            _this.list = data
                         

                        }
                    }
                }
            },
            created: function () {


                this.fun()
            }
        })



        var i1 = 0;
        var old1 = -1;
        var new1 = -2;
        setInterval(function () {
            var table1 = $('#card-body1')
            if (table1.length == 0) {
                return;
            }
            $(table1).scrollTop(i1);
            new1 = $(table1).scrollTop();
            if (old1 == new1) {
                i1 = 0;
                old1 = -1;
                return;
            }
            i1 += 1.2;
            old1 = $(table1).scrollTop();
        }, 200);




        var i2 = 0;
        var old2 = -1;
        var new2 = -2;
        setInterval(function () {
            var table2 = $('#card-body2')
            if (table2.length == 0) {
                return;
            }
            $(table2).scrollTop(i2);
            new2 = $(table2).scrollTop();
            if (old2 == new2) {
                i2 = 0;
                old2 = -1;
                return;
            }
            i2 += 1.2;
            old2 = $(table2).scrollTop();
        }, 200);




    </script>
