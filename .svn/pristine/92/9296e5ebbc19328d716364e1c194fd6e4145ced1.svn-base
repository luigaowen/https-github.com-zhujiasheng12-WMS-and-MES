            }
        })



        layui.use('table', function () {
            var table = layui.table;

            //第一个实例
        

          var i1 = 0;
        var old1 = -1;
        var new1 = -2;
            setInterval(function () {
              var table1=  $('#card-body1').find('.layui-table-body.layui-table-main')
            if (table1.length == 0) {
                return;
            }
            $(table1).scrollTop(i1);
            new1 = $(table1).scrollTop();
            if (old1 == new1) {
                i1 = 0;
                old1 = -1;
                return;
            }
            i1 +=1.2;
            old1 = $(table1).scrollTop();
        }, 200);

        setInterval(function () {
            tableReload1.reload()
        }, 60000)


             var i2 = 0;
        var old2 = -1;
        var new2 = -2;
            setInterval(function () {
              var table2=  $('#card-body1').find('.layui-table-body.layui-table-main')
            if (table2.length == 0) {
                return;
            }
            $(table2).scrollTop(i2);
            new2 = $(table2).scrollTop();
            if (old2 == new2) {
                i2 = 0;
                old2 = -1;
                return;
            }
            i2+=1.2;
            old2 = $(table2).scrollTop();
        }, 200);

        setInterval(function () {
            tableReload2.reload()
        }, 60000)
        });


    </script>
